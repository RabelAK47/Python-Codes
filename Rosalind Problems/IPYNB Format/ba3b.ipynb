    "F = open('D:/input_test.txt', 'r')\n",
    "lines = []\n",
    "ans = \"\"\n",
    "for L in F.readlines():\n",
    "    lines.append(L.strip())\n",
    "for i in range(len(lines)-1):\n",
    "    temp = lines[i]\n",
    "    ans += temp[0]\n",
    "ans = ans + lines[len(lines)-1]\n",
    "print(ans)\n",
    "ans = \"\"\n",
    "F.close()"
   ]
